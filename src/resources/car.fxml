<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="427.0" prefWidth="288.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CarController">
 <Label text="Year:">
      <font>
         <Font size="16.0" />
      </font></Label>
 <TextField fx:id="yearField" />
 <Label text="License number:">
      <font>
         <Font size="16.0" />
      </font></Label>
 <TextField fx:id="licenseNumberField" />
 <Label text="Model:">
      <font>
         <Font size="16.0" />
      </font></Label>
 <TextField fx:id="modelField" />
 <Label text="Fuel type:">
      <font>
         <Font size="16.0" />
      </font></Label>
 <ComboBox fx:id="fuelTypeBox" prefHeight="25.0" prefWidth="291.0" />
 <Label text="Fuel consumption:">
      <font>
         <Font size="16.0" />
      </font></Label>
 <TextField fx:id="fuelConsumptionField" />
 <Label text="Fuel tank capacity">
      <font>
         <Font size="16.0" />
      </font></Label>
 <TextField fx:id="fuelTankCapacityField" />
 <Label text="Current fuel level:">
      <font>
         <Font size="16.0" />
      </font></Label>
 <TextField fx:id="currentFuelLevelField" />
 <Button fx:id="submitButton" onAction="#handleSubmitButtonAction" prefHeight="83.0" prefWidth="288.0" text="Submit">
      <font>
         <Font name="System Bold" size="16.0" />
      </font></Button>
</VBox>
