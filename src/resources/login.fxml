<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="233.0" prefWidth="373.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="resources.LoginController">
   <children>
      <Label layoutX="146.0" layoutY="40.0" prefHeight="27.0" prefWidth="93.0" text="Login Form" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Label layoutX="51.0" layoutY="81.0" text="Username:" />
      <TextField fx:id="userNameTextField" layoutX="118.0" layoutY="77.0" promptText="insert username" />
      <Label layoutX="55.0" layoutY="120.0" text="Password:" />
      <PasswordField fx:id="passwordTextField" layoutX="118.0" layoutY="117.0" promptText="insert password" />
      <Button fx:id="loginButton" layoutX="167.0" layoutY="150.0" mnemonicParsing="false" onAction="#login" onKeyPressed="#login" text="Login" textAlignment="CENTER" />
      <Button fx:id="registerButton" layoutX="161.0" layoutY="187.0" mnemonicParsing="false" onAction="#register" text="Register" />
   </children>
</AnchorPane>
